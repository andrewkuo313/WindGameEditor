<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" stylesheets="@Page.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gmail.andrewchouhs.controller.MainPageController">
   <children>
      <BorderPane fx:id="mainPage" prefHeight="200.0" prefWidth="200.0" styleClass="MainPageBorderPane" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <left>
            <VBox prefHeight="200.0" prefWidth="100.0" styleClass="MainPageMenu" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onMouseClicked="#toFilePage" prefHeight="50.0" prefWidth="100.0" styleClass="MainPageButton" text="檔案管理" />
                  <Button mnemonicParsing="false" onMouseClicked="#toGamePage" prefHeight="50.0" prefWidth="100.0" styleClass="MainPageButton" text="遊戲屬性" />
                  <Button mnemonicParsing="false" onMouseClicked="#toTaskPage" prefHeight="50.0" prefWidth="100.0" styleClass="MainPageButton" text="任務列表" />
                  <Button mnemonicParsing="false" onMouseClicked="#toItemPage" prefHeight="50.0" prefWidth="100.0" styleClass="MainPageButton" text="道具設定" />
               </children>
            </VBox>
         </left>
      </BorderPane>
   </children>
</AnchorPane>
